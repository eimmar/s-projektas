<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{ 'main.header'|trans }}!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/css/main.css') }}">
        {% endblock %}
    </head>
    <body>
    {{ include('includes/navbar.html.twig') }}
    <div class="container">

        {{ include('includes/flashes.html.twig') }}

        <h1 class="card-header h1">
            {% block mainHeader %}{% endblock %}
        </h1>

        {% block body %}{% endblock %}
    </div>
    {{ include('includes/footer.html.twig') }}
        {% block javascripts %}
            <script src="{{ asset('build/js/main.js') }}"></script>
        {% endblock %}
    </body>
</html>
