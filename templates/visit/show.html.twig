{% extends 'base.html.twig' %}

{% block title %}{{ 'visit_index'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'visit_index'|trans }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ visit.id }}</td>
            </tr>
            <tr>
                <th>VisitDate</th>
                <td>{{ visit.visitDate ? visit.visitDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateCreated</th>
                <td>{{ visit.dateCreated ? visit.dateCreated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateUpdated</th>
                <td>{{ visit.dateUpdated ? visit.dateUpdated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>TotalInclTax</th>
                <td>{{ visit.totalInclTax }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('visit_index') }}">{{ 'visit_index'|trans }}</a>

    <a href="{{ path('visit_edit', {'id': visit.id}) }}">{{ 'visit.action.edit'|trans }}</a>

    {{ include('visit/_delete_form.html.twig') }}
{% endblock %}